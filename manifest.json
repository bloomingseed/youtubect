{
    "manifest_version": 3,
    "name": "Youtube Commentor Tool",
    "description": "A tool to auto post comment to youtube video and return the comment URL. This extension works together with the designed Google app script.",
    "version": "1.0.0",
    "background":{
        "service_worker": "background.js"
    },
    "icons":{
        "16": "/images/logo_16.png",
        "128":"/images/logo_128.png"
    },
    "action":{
        "default_icon":{
            "16": "/images/logo_16.png",
            "128":"/images/logo_128.png"
        },
        "default_popup":"/popup.html"
    },
    "permissions":[
        "tabs",
        "scripting",
        "storage",
        "identity"
    ],
    "host_permissions":[
        "https://www.youtube.com/*"
    ],
    "oauth2": {
        "client_id": "99161837988-smooula9ggb1568q2jona5oernsua5ng.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/spreadsheets"
        ]
    },
    "key":""
}